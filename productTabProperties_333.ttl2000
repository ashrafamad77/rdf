@prefix schema:   <http://schema.org/> .
@prefix gr:       <http://purl.org/goodrelations/v1#> .
@prefix xsd:      <http://www.w3.org/2001/XMLSchema#> .
@prefix findme:   <http://www.findme.com/ontology#> .
@prefix dul:      <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .

:thisProduct  a schema:Product ;

    # 📏 Physical Dimensions
    schema:height           "55"^^xsd:decimal ;
    schema:width            "170"^^xsd:decimal ;
    schema:depth            "30"^^xsd:decimal ;
    schema:weight           "140"^^xsd:decimal ;

    # 🖥 Display
    findme:displayType       "Niveau de gris" ;
    findme:screenResolution  "128x160" ;

    # ☎️ Telephony Features
    findme:hasPhonebook         true ;
    findme:phonebookEntries     "150"^^xsd:decimal ;
    findme:hasAnsweringMachine  true ;
    findme:recordingTime        "25"^^xsd:decimal ;
    findme:supportsVoIP         false ;
    findme:voipTypes            () ;

    # 🔊 Audio & Utility
    findme:hasSpeakerphone   true ;
    findme:hasBabyMonitor    true ;
    findme:supportsSMS       true ;

    # 🔋 Battery & Connectivity
    findme:standbyTime       "320"^^xsd:decimal ;
    findme:talkTime          "14"^^xsd:decimal ;
    findme:hasBluetooth      false ;

    # ✨ Lighting
    findme:hasBacklitKeys     true ;
    findme:hasBacklitDisplay  true ;

    # 🎨 Design
    schema:color  "Noir", "Argent", "Gris" ;

    # 🔧 Features (List)
    findme:feature  "Bloqueur d'appels" ;

    # 🔌 Compatibility
    findme:isGAPCompatible  true ;

    # 🧠 Core Typology and Extensions
    findme:productType   "Dect" ;
    findme:ecoDect       true ;
    findme:ecoDectPlus   false ;
    findme:sipDect       false .
